<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="footer">
            <block class="Magento\Cms\Block\Block" name="Curso-Hiberus-2021" after="-">
                <arguments>
                    <argument name="block_id" xsi:type="string">Curso-Hiberus-2021</argument>
                </arguments>
            </block>
        </referenceContainer>
        <referenceContainer name="footer">
            <block class="Magento\Theme\Block\Html\Header\Logo" name="footer.logo" before="-"/>
            <block class="Magento\Cms\Block\Block" name="footer.custom" after="-">
                <arguments>
                    <argument name="block_id" xsi:type="string">footer-custom</argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
